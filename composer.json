{
    "name": "qubiqx/qcommerce-core",
    "description": "Qcommerce Core",
    "keywords": [
        "Qubiqx",
        "laravel",
        "qcommerce-core"
    ],
    "homepage": "https://github.com/qubiqx/qcommerce-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@qubiqx.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Qubiqx\\QcommerceCore\\": "src",
            "Qubiqx\\QcommerceCore\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "../qcommerce-core/src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Qubiqx\\QcommerceCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qubiqx\\QcommerceCore\\QcommerceCoreServiceProvider"
            ],
            "aliases": {
                "QcommerceCore": "Qubiqx\\QcommerceCore\\Facades\\QcommerceCore",
                "Sitemap": "Qubiqx\\QcommerceCore\\Classes\\Sitemap",
                "Helper": "Qubiqx\\QcommerceCore\\Classes\\Helper",
                "Locales": "Qubiqx\\QcommerceCore\\Classes\\Locales",
                "Forms": "Qubiqx\\QcommerceCore\\Classes\\Forms",
                "Menus": "Qubiqx\\QcommerceCore\\Classes\\Menus",
                "Webshop": "Qubiqx\\QcommerceCore\\Classes\\Webshop",
                "Sites": "Qubiqx\\QcommerceCore\\Classes\\Sites",
                "Translation": "Qubiqx\\QcommerceCore\\Models\\Translation",
                "Customsetting": "Qubiqx\\QcommerceCore\\Models\\Customsetting",
                "Product": "Qubiqx\\QcommerceCore\\Models\\Product",
                "Products": "Qubiqx\\QcommerceCore\\Classes\\Products",
                "Articles": "Qubiqx\\QcommerceCore\\Classes\\Articles",
                "ProductCategories": "Qubiqx\\QcommerceCore\\Classes\\ProductCategories",
                "ShoppingCart": "Qubiqx\\QcommerceCore\\Classes\\ShoppingCart",
                "ShippingZones": "Qubiqx\\QcommerceCore\\Classes\\ShippingZones",
                "SKUs": "Qubiqx\\QcommerceCore\\Classes\\SKUs"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
