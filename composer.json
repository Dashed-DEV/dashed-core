{
    "name": "qubiqx/qcommerce-core",
    "description": "Qcommerce Core",
    "keywords": [
        "Qubiqx",
        "laravel",
        "qcommerce-core"
    ],
    "homepage": "https://github.com/qubiqx/qcommerce-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@qubiqx.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "awcodes/filament-tiptap-editor": "^2.6",
        "doctrine/dbal": "^3.3",
        "filament/filament": "^2.17",
        "filament/spatie-laravel-translatable-plugin": "^2.17",
        "flowframe/laravel-drift": "^0.2.0",
        "laravel/horizon": "^5.18",
        "mcamara/laravel-localization": "^1.8",
        "mohamedsabil83/filament-forms-tinyeditor": "^1.7",
        "psr/simple-cache": "3.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-ignition": "^2.2",
        "spatie/laravel-package-tools": "^1.15",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/schema-org": "^3.14",
        "webbingbrasil/filament-2fa": "^1.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Qubiqx\\QcommerceCore\\": "src",
            "Qubiqx\\QcommerceCore\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Qubiqx\\QcommerceCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qubiqx\\QcommerceCore\\QcommerceCoreServiceProvider"
            ],
            "aliases": {
                "AccountHelper": "Qubiqx\\QcommerceCore\\Classes\\AccountHelper",
                "LinkHelper": "Qubiqx\\QcommerceCore\\Classes\\LinkHelper",
                "Helper": "Qubiqx\\QcommerceCore\\Classes\\Helper",
                "Locales": "Qubiqx\\QcommerceCore\\Classes\\Locales",
                "Sites": "Qubiqx\\QcommerceCore\\Classes\\Sites",
                "Customsetting": "Qubiqx\\QcommerceCore\\Models\\Customsetting"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
